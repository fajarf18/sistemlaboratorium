{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images|storage)/(.*)",
      "dest": "public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://sistemlaboratorium-git-main-fajarf18s-projects.vercel.app",
    "APP_KEY": "@app_key",
    "DATABASE_URL": "@database_url",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "database",
    "CACHE_STORE": "database"
  }
}